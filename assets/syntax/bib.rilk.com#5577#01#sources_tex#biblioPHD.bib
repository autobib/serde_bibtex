@ARTICLE{Gabrielle2002b,
  title = {{A priori parameterisation of the {CERES} soil-crop models and tests against several European data sets}},
  author = {Gabrielle, B. and Roche, R. and Angas, P. and Cantero-Martinez, C. and Cosentino, L. and Mantineo, M. and Langensiepen, M. and Hénault, C. and Laville, P. and Nicoullaud, B. and Gosse, G.},
  journal = {Agronomie},
