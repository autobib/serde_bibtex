}

@ARTICLE{Conen2000,
  title = {Predicting {N$_2$O} emissions from agricultural land through related soil parameters},
  author = {Conen, F. and Dobbie, K. E. and Smith, K. A.},
  journal = {Global Change Biology},
  year = {2000},
