@article{Cohen.Kanza.ea_EquiX-asearchand_JASIST_2002,
	Author = {Cohen, Sara and Kanza, Yaron and Kogan, Yakov and Sagiv, Yehoshua and Nutt, Werner and Serebrenik, Alexander},
	Date-Added = {2007-03-12 23:03:36 +0100},
