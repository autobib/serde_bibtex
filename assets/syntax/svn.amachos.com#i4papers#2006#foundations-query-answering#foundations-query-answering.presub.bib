@inproceedings{Neven2002XPath-Containment,
	Address = {London, UK},
	Author = {Neven, Frank and Schwentick, Thomas},
	Booktitle = {Proc. Int. Conf. on Database Theory},
	Date-Added = {2007-03-12 23:03:36 +0100},
