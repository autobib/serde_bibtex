	Booktitle = {Proc. Australasian Conf. on Database Technologies},
	Conference-Abbr = {CRPITS},
	Date-Added = {2007-03-12 23:03:36 +0100},
	Date-Modified = {2007-03-22 09:36:21 +0100},
