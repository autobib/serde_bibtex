@article{Miklau2004Containment-Equivalence-Fragment-of-XPath,
	Address = {New York, NY, USA},
	Author = {Miklau, Gerome and Suciu, Dan},
	Date-Added = {2007-03-12 23:03:36 +0100},
	Date-Modified = {2007-03-22 09:36:21 +0100},
