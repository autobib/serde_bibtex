	Date-Modified = {2007-03-22 09:36:21 +0100},
	Keywords = {personalization, E-Learning, Personal Reader, Semantic Web, RDF},
	Owner = {Tim Furche},
	Pdf = {ApplicationAreas/Personalization/Dolog.Henze.ea_PersonalReader:Personalizing_AH_2004.pdf},
