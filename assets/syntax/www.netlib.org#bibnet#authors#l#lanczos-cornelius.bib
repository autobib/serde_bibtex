@Article{Lanczos:1976:FAR,
  author =       "Cornelius Lanczos and Barbara Gellai",
  title =        "{Fourier} analysis of random sequences",
  journal =      j-COMP-MATH-APPL,
  volume =       "1",
  number =       "3--4",
  pages =        "269--276",
  year =         "1976",
  CODEN =        "CMAPDK",
  DOI =          "http://dx.doi.org/10.1016/0898-1221(75)90025-5",
