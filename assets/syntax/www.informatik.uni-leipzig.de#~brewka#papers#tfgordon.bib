@misc{Wulff:2000,
	Abstract = {Nicht mehr die Frage, ob die Kommune Internet-Technologie nutzen sollte, treibt um, sondern wie dies geschehen soll. Doch geht es bei dem Wie nicht nur um technische Probleme und organisatorische Regeln, die man vielerorts diskutiert. Vielmehr stellt sich die Frage, wie die M{\"o}glichkeiten der Internet-Technologie ins strategische Management der Kommune einzubinden sind, welche Ziele die Technologie unterst{\"u}tzen soll, welche Schwerpunkte man auf der Leistungsebene setzen will, wie Umsetzung und wie Steuerung aussehen k{\"o}nnte. Dieser strategischen Dimension, die die Verantwortung der Verwaltungsf{\"u}hrung betrifft, weichen noch etliche Kommunen aus. Hier bietet der Bericht Hilfen an, indem er mobilisierende Leistungsverst{\"a}rker und Entwicklungschancen beschreibt, Zusammenh{\"a}nge zwischen strategischem Management und Verf{\"u}gbarkeit der Internet-Technologie aufzeigt, Inhalte einer `Internet-Strategie' skizziert, Fragen der Verantwortung diskutiert, neue M{\"o}glichkeiten der Prozessgestaltung vorstellt, Leistungsvarianten anbietet und Schritte zur Umsetzung darstellt. In dem Bericht geht es um die Einbindung der M{\"o}glichkeiten des Internet in die kommunale Strategie und um die Ausgestaltung der Internet-Nutzung vor Ort. Denn nur wenn man sich neben den operativen Fragen auch den strategischen Managementfragen stellt und die Steuerung sicherstellt, wird die Internet-Nutzung dazu beitragen, Zielgenauigkeit, Effizienz und Qualit{\"a}t kommunaler Dienstleistungen zu verbessern.},
	Author = {Wulff, Marianne},
	Date-Modified = {2005-03-29 16:15:43 +0200},
	Keywords = {e-government},
	Publisher = {KGSt: Verband f{\"u}r kommunales Management},
	Title = {Kommune und Internet: Strategische {\"U}berlegungen und Hilfen zur Umsetzung},
	Year = {2000}}

