@article{Rinner:1996,
	Abstract = {Geographische Informationssysteme (GIS) entwickeln sich mehr und mehr zu   Universalwerkzeugen fr die Umweltforschung und Umweltplanung. Die rumlich   und zeitlich dynamischen Vorgnge in der Umwelt lassen sich jedoch in den   Datenstrukturen der Standard-GIS-Software nicht umfassend abbilden. Neben der   Weiterentwicklung der eigentlichen GIS-Technologie werden in Kombination mit   GIS zunehmend Umweltsimulationsmodelle eingesetzt, die natrliche Prozesse   mathematisch berechnen. In diesem Kontext versucht die vorliegende Arbeit   zunchst einen berblick ber die vorhandenen Anstze im Bereich 'GIS Aided   Modelling' zu geben. Der Schwerpunkt der anschlieenden Untersuchung liegt   auf der Funktion des GIS fr die Datenvor- und Datennachbereitung und auf dem   Ablauf einer mengefhrten Simulation aus Sicht des Anwenders eines   integrierten Simulationssystems. Fr das GIS Arc/Info und fr die Anwendung   in der Simulation des Schadstofftransports in Fliegewssern wird eine - in   der Arc Macro Language (AML) implementierte - Benutzeroberflche eines   einfachen GIS-Modell-Systems vorgestellt, das mit dem Modell WATER und den   Daten des regionalen Flusystems der Hase im Landkreis Osnabrck getestet   wurde. Beim Aufbau der raumbezogenen Datenbasis wurde das Arc/Info-Modul   Dynamic Segmentation genutzt. Die Ergebnisse der Arbeit werden in das   internationale GREAT-ER-Projekt einflieen, das die derzeit in der   Europischen Union verwendeten Methoden fr die Vorhersage der lokalen und   regionalen Konzentration chemischer Substanzen in Fliegewssern verbessern   soll.},
	Author = {Rinner, Claus},
	Date-Modified = {2005-03-29 16:15:44 +0200},
	Title = {Einsatz eines Geographischen Informationssystems bei der Simulation von Schadstoffen in Flie{\ss}gew{\"a}ssern},
	Volume = {April},
	Year = {1996}}

