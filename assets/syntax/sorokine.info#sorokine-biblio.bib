@article{vinogradov98:_mappin_ecolog_unfav_zones_use_dynam_criter,
  author = {Boris V Vinogradov and K N Kulik and Alexandre Sorokine and P B Fedotov},
  title = {Mapping of Ecologically Unfavorable Zones with the Use of Dynamic Criteria},
  journal = {Russian Journal of Ecology},
  volume = {29},
  number = {4},
  pages = {209--216},
  note = {Russian edition: Ekologiya 1998, N 4, pp. 243-251},
  abstract = {The isopleth method for constructing dynamic maps of ecologically unfavorable zones was developed and applied to mapping the Chernye Zemli (the Kalmyk Republic) with the use of the Kriging method. The maps were based on the data from series of aerial and space photographs made in 1954, 1964, 1979, 1984, and 1993. Dynamic criteria were used. On the basis of isolinear dynamic maps for these periods of time, maps of the ecological state were constructed using the dynamic criteria. In these maps, the ecological state is classified into norm, risk, crisis, and disaster.},
  url = {http://www.maik.rssi.ru/journals/ecol.htm},
  year = {1998},
  cat = {research}
}

