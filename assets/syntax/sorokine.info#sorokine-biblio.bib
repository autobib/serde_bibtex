@article{vinogradov99:_isody,
  author = {Boris V Vinogradov and K N Kulik and Alexandre Sorokine and P B Fedotov},
  title = {Isodynamic mapping and long-term monitoring of desertification and degradation of lands using nonlinear methods of modeling},
  journal = {Eurasian Soil Science},
  volume = {32},
  number = {4},
  pages = {449-458},
  month = {April},
  abstract = {The method is based on comparison of the results of land cover assessment for a long period of time (30-40 years) using aerial and satellite surveys performed at intervals of 5-10 years. Analysis of characteristics of land cover by the kriging method made it possible to compile maps of the change rates (the first derivative) and maps of change acceleration (the second derivative). Comparison of these maps permitted determination of the area, distribution, and rate of adverse changes in the environment; location of zones of risk, crisis, and disaster; as well as the location and time of onset and recession of land degradation processes.},
  url = {http://www.maik.rssi.ru/journals/soilsci.htm},
  year = {1999},
  cat = {research}
}

