@Article{Granat:2010:PSS,
  author =       "Robert Granat and Bo Kagstrom",
  title =        "Parallel Solvers for {Sylvester}-Type Matrix Equations
                 with Applications in Condition Estimation, Part {I}:
                 Theory and Algorithms",
  journal =      j-TOMS,
  volume =       "37",
  number =       "3",
  pages =        "32:1--32:32",
