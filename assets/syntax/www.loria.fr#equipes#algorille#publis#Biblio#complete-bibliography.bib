@INPROCEEDINGS{caniou04b,
   AUTHOR       = {Yves Caniou and Emmanuel Jeannot},
   BOOKTITLE    = {QOS and Dynamic System Workshop of ICPADS'2004 - 10th International Conference on Parallel and Distributed Systems},
   TITLE        = {Efficient Scheduling Heuristics for GridRPC Systems},
   YEAR         = {2004},
   ADDRESS      = {New-Port Beach, CA},
   OPTCROSSREF  = {},
   OPTEDITOR    = {},
   MONTH        = {July},
   OPTNOTE      = {},
   OPTNUMBER    = {},
   OPTORGANIZATION = {},
   PAGES        = {621--630},
   PUBLISHER    = {IEEE},
   OPTSERIES    = {},
   OPTVOLUME    = {},
   KEYWORDS     = {HTM, QoS, dynamic scheduling, heterogeneity},
   ABSTRACT     = {In this paper we examine several scheduling heuristics 
      for GridRPC middleware relying on the time-shared model (a server can 
      execute more than one task at a time). Our work is based on a 
      forecast module called the Historical Trace Manager (HTM), which is 
      able to predict durations of tasks in the system. We show that the 
      predictions performed by the HTM are very accurate. The five proposed 
      scheduling heuristics use these predictions to map submitted tasks to 
      servers. Experimental simulation results show that they are able to 
      outperform the well-known MCT heuristic for several metrics (makespan 
      but also sumflow, max-stretch, etc.) and therefore provide a better 
      quality of service for the client.}
}

@INPROCEEDINGS{caniou04a,
   AUTHOR       = {Yves Caniou and Emmanuel Jeannot},
   BOOKTITLE    = {Proceedings of the 10th International Euro-Par Conference (Euro-Par'04)},
   TITLE        = {Experimental Study of Multi-Criteria Scheduling 
      Heuristics for GridRPC Systems},
   YEAR         = {2004},
   ADDRESS      = {Pisa, Italy},
   OPTCROSSREF  = {},
   EDITOR       = {Marco Danelutto and Domenico Laforenza and 
      Marco Vanneschi},
   MONTH        = {August/September},
   OPTNOTE      = {},
   OPTNUMBER    = {},
   OPTORGANIZATION = {},
   PAGES        = {1048--1055},
   PUBLISHER    = {Springer},
   SERIES       = {Lecture Notes in Computer Science},
   VOLUME       = {3149},
   KEYWORDS     = {time-shared and heterogeneous resources, 
      dynamic scheduling heuristics, historical trace manager, 
      completion dates estimations, dags},
   URL          = {http://springerlink.metapress.com/openurl.asp?genre=article&issn=0302-9743&volume=3149&spage=1048},
   ABSTRACT     = {We study in this paper several scheduling heuristics for 
      GridRPC middlewares. When dealing with performance issue, the 
      scheduling strategy is one of the most important feature. However, 
      many heuristics implemented in available middlewares were not 
      designed for this context (for instance MCT in NetSolve). Therefore, 
      schedulers are not necessarily effective. We propose to use 
      heuristics based on a non-intrusive module which is able to estimate 
      the duration of all tasks in the system. Three criteria are examined 
      among which the execution time of the application, e.g. the makespan. 
      Experiments on a real platform show that the proposed heuristics 
      outperform MCT for at least two of these three criteria.}
}

