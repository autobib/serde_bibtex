@Article{Crawford:1997:NTP,
  author =       "Elisabeth Crawford and Ruth Lewin Sime and Mark
                 Walker",
  title =        "A {Nobel} Tale of Postwar Injustice",
  journal =      j-PHYS-TODAY,
  volume =       "50",
