  title =        "Uncertainty: {Einstein}, {Heisenberg}, {Bohr}, and the
                 struggle for the soul of science",
  publisher =    pub-ANCHOR-BOOKS,
  address =      pub-ANCHOR-BOOKS:adr,
  pages =        "viii + 257",
