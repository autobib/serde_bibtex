@article{la1915,
    AUTHOR = {Lasker, E.},
title="{\"Uber das mathematisch {S}ch\"one.}",
   JOURNAL = {Mathematisch naurwissenschaftliche {B}l\"atter},
    VOLUME = 12,
year=1915,
    PAGES = {49--53},
abstract={``Sch\"one S\"atze haben die Eigenart, da{\ss} sie unsere Neugierde
    erregen, sie sodann auf leichte, durchsichtige, z. B. anschauliche Art
    befriedigen und zu einem Ruhepunkte f\"uhren, wo der Geist sich behaglich
    aufzuhalten vermag. Sch\"one Beweise haben sie Eigenart, da{\ss} sie sowohl
    sparsam sind an Mitteln wie auch aufs \"au{\ss}erste charakteristisch. Mit
    dieser Feststellung ist so viel gewonnen, da{\ss} sie uns sowohl hilft, wenn
    wir den \"uberlieferten Schatz der Theorie ordnen, klassifizieren,
    weitergeben wollen, wie auch, wenn wir ihn vermehren wollen durch
    erfinderische T\"atigkeit.''\par Dieses Ergebnis des Vortrages wird auf
    induktivem Wege aus der Betrachtung einer Reihe treffender Beispiele
    gewonnen.}
}

@article{la1916,
    AUTHOR = {Lasker, E.},
title= {{\"U}ber eine {E}igenschaft der   {D}iskriminante.},
language="German",
journal={Sitzungsber. Berl. Math. Ges. },
volume=15,
pages={176--178},
year=1916,
abstract={Der Verf. beweist auf einfachem Wege den folgenden Satz von {\it
    Vorono\"\i}: Ist $f(x)$ ein beliebiges ganzzahliges Polynomen vom Grade $n$
    und $D$ dessen Diskriminante, $p$ eine ungerade, zu $D$ teilerfremde
    Primzahl, zerf\"allt $f(x) \text{mod.} p$ in eine Anzahl $a$ verschiedener
    Teiler, so ist $$ \left( \frac {D}{P}\right) =(-1)^{n+a}.$$ Dieser Satz wird
    angewandt auf das Polynom $x^n-1$, wo $n$ eine Primzahl ist, und hieraus
    eine einfache Ableitung des Reziprozit\"atsgesetzes der quadratischen Reste
    gegeben.},
}


