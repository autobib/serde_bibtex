  pages =        "316--330",
  year =         "1992",
  bibdate =      "Wed Oct 25 12:40:13 MDT 2000",
  bibsource =    "http://www.acm.org/pubs/contents/proceedings/series/sigmod_pods/",
  URL =          "http://www.acm.org/pubs/articles/proceedings/pods/137097/p316-bonner/p316-bonner.pdf;
