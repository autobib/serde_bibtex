@InProceedings{Aly:1987:NDM,
  author =       "Hussien Aly and Z. Meral Ozsoyoglu",
  title =        "Non-deterministic modelling of logical queries in
                 deductive databases",
  crossref =     "Dayal:1987:PAC",
  pages =        "60--72",
