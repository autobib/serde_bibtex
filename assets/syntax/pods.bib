@InProceedings{Cruz:1992:DVL,
  author =       "Isabel F. Cruz",
  title =        "{DOODLE}: a visual language for object-oriented
                 databases",
  crossref =     "Stonebraker:1992:PAS",
  pages =        "71--80",
  year =         "1992",
  bibdate =      "Wed Oct 25 12:40:13 MDT 2000",
