@InProceedings{Polyzois:1992:ERB,
  author =       "Christos A. Polyzois and Hector Garcia-Molina",
  title =        "Evaluation of remote backup algorithms for transaction
                 processing systems",
  crossref =     "Stonebraker:1992:PAS",
  pages =        "246--255",
